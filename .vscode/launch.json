{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Expo (Chrome DevTools)",
      "type": "reactnative",
      "request": "launch",
      "platform": "exponent",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Debug iOS",
      "type": "reactnative",
      "request": "launch",
      "platform": "ios",
      "target": "simulator"
    },
    {
      "name": "Debug Android",
      "type": "reactnative",
      "request": "launch",
      "platform": "android"
    },
    {
      "name": "Attach to Expo",
      "type": "reactnative",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}